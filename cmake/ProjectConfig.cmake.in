IF(WIN32)
	GET_FILENAME_COMPONENT(_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
	SET(IMPORT_PATH "${_DIR}/@EXPORT_NAME@.cmake")
	GET_FILENAME_COMPONENT(@PROJECT_NAME@_ROOT_DIR "${_DIR}" PATH)
ELSE()
	SET(IMPORT_PATH "@IMPORT_PATH@")
	SET(@PROJECT_NAME@_ROOT_DIR "@CMAKE_INSTALL_PREFIX@")
ENDIF()
IF(@USE_IMPORT@)
	INCLUDE("${IMPORT_PATH}")
ENDIF()
SET(@PROJECT_NAME@_INCLUDE_DIRS
	"${@PROJECT_NAME@_ROOT_DIR}/include"
	"${@PROJECT_NAME@_ROOT_DIR}/include/@PROJECT_NAME@"
)
SET(@PROJECT_NAME@_HTMLDOC_DIR
	"${@PROJECT_NAME@_ROOT_DIR}/@PROJECT_INSTALL_DOC@/html"
	CACHE PATH "path to doxygen generated html documentation (may be URL)"
)
SET(@PROJECT_NAME@_TAGFILE
	"${@PROJECT_NAME@_ROOT_DIR}/@PROJECT_INSTALL_DOC@/@PROJECT_NAME@.tag"
	CACHE FILEPATH "doxygen tag import file" FORCE
)
SET(@PROJECT_NAME@_TAG_IMPORT
	"\"${@PROJECT_NAME@_TAGFILE}=${@PROJECT_NAME@_HTMLDOC_DIR}\""
)
MARK_AS_ADVANCED(
	@PROJECT_NAME@_DIR
	@PROJECT_NAME@_HTMLDOC_DIR
	@PROJECT_NAME@_TAGFILE
)
